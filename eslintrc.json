{
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "quotes": ["error", "double"],
      "indent": ["error", 2],
      "max-len": ["error", { "code": 80 }],
      "comma-dangle": ["error", "always-multiline"],
      "object-curly-spacing": ["error", "never"]
    }
  }
  